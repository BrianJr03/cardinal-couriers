        <Button layoutX="323.0" layoutY="478.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="97.0" styleClass="logoutBtn" stylesheets="@../stylesheets/mainUI_stylesheet.css" text="Checkout">
