<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.bsu.cs222.finalProject.Controllers.CartController">
    <Pane layoutX="-3.0" layoutY="-1.0" prefHeight="623.0" prefWidth="739.0" styleClass="parent">
        <Label layoutX="128.0" layoutY="89.0" text="Cart" textFill="DARKORCHID">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <TableView layoutX="128.0" layoutY="124.0" prefHeight="200.0" prefWidth="487.0">
            <columns>
                <TableColumn minWidth="236.0" prefWidth="75.0" text="Item" />
                <TableColumn minWidth="133.60003662109375" prefWidth="75.0" text="Price" />
                <TableColumn minWidth="117.4000244140625" prefWidth="75.0" text="Quantity" />
